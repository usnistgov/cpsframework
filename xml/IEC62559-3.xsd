	<xs:complexType name="Version">
		<xs:annotation>
			<xs:documentation>Version of the use case(s) description.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="approvalStatus" type="xs:string" minOccurs="0" maxOccurs="1">
