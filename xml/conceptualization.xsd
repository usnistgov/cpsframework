<?xml version="1.0" encoding="utf-8"?>
<!-- edited with XMLSpy v2020 rel. 2 sp1 (http://www.altova.com) by Martin Burns (National Institute of Standards and Technology) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="cpsframework" targetNamespace="cpsframework">
	<xs:annotation>
		<xs:documentation>This package contains definitions for the conceptualization facet.</xs:documentation>
	</xs:annotation>
	<xs:include schemaLocation="cpsframework.xsd"/>
	<xs:include schemaLocation="activities.xsd"/>
	<xs:element name="Actuation" type="Actuation"/>
	<xs:complexType name="Actuation">
		<xs:annotation>
			<xs:documentation>Concerns related to the ability of the CPS to effect change in the physical world.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Adaptability" type="Adaptability"/>
	<xs:complexType name="Adaptability">
		<xs:annotation>
			<xs:documentation>Concerns about the ability of the CPS to achieve an intended purpose in the face of changing external conditions such as the need to upgrade or otherwise reconfigure a CPS to meet new conditions, needs, or objectives.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Aggregation" type="Aggregation"/>
	<xs:complexType name="Aggregation">
		<xs:annotation>
			<xs:documentation>Concerns about the aggregation of data to form summaries or collections from multiple source</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Analytics" type="Analytics"/>
	<xs:complexType name="Analytics">
		<xs:annotation>
			<xs:documentation>Concerns about analytics</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Authentication" type="Authentication"/>
	<xs:complexType name="Authentication">
		<xs:annotation>
			<xs:documentation>Concerns about the authenticity of data</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Authorization" type="Authorization"/>
	<xs:complexType name="Authorization">
		<xs:annotation>
			<xs:documentation>Concerns about the authorizations constraining access to data</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Availability" type="Availability"/>
	<xs:complexType name="Availability">
		<xs:annotation>
			<xs:documentation>Concerns about ensuring timely and reliable access to and use of a system.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BasicConnectivity" type="BasicConnectivity"/>
	<xs:complexType name="BasicConnectivity">
		<xs:annotation>
			<xs:documentation>Concerns about connectivity of CPSDevices</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence>
					<xs:element name="OSIDataLink" type="OSIDataLink" minOccurs="0" maxOccurs="1"/>
					<xs:element name="OSIPhysical" type="OSIPhysical" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Behavioral" type="Behavioral"/>
	<xs:complexType name="Behavioral">
		<xs:annotation>
			<xs:documentation>Concerns about interdependence among behavioral domains. Concerns related to the ability to successfully operate a CPS in multiple application areas.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BOL" type="BOL"/>
	<xs:complexType name="BOL">
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence>
					<xs:element name="Deployability" type="Deployability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Engineerability" type="Engineerability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Procurability" type="Procurability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Producibility" type="Producibility" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BoundariesAspect" type="BoundariesAspect"/>
	<xs:complexType name="BoundariesAspect">
		<xs:annotation>
			<xs:documentation>Concerns about the demarcations of topological, functional, organizational, or other forms of interactions.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Aspect">
				<xs:sequence>
					<xs:element name="Behavioral" type="Behavioral" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Networkability" type="Networkability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Responsibility" type="Responsibility" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BusinessAspect" type="BusinessAspect"/>
	<xs:complexType name="BusinessAspect">
		<xs:annotation>
			<xs:documentation>Concerns about enterprise, time to market, environment, regulation, cost, etc.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Aspect">
				<xs:sequence>
					<xs:element name="Cost" type="Cost" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Enterprise" type="Enterprise" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Environment" type="Environment" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Planning" type="Planning" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Policy" type="Policy" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Quality" type="Quality" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Regulatory" type="Regulatory" minOccurs="0" maxOccurs="1"/>
					<xs:element name="TimeToMarket" type="TimeToMarket" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Utility" type="Utility" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="ClassIdentification" type="ClassIdentification"/>
	<xs:complexType name="ClassIdentification">
		<xs:annotation>
			<xs:documentation>Concerns about identification of data representing classes</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Communication" type="Communication"/>
	<xs:complexType name="Communication">
		<xs:annotation>
			<xs:documentation>Concerns related to the exchange of information internal to the CPS and between the CPS and other entities.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence>
					<xs:element name="BasicConnectivity" type="BasicConnectivity" minOccurs="0" maxOccurs="1"/>
					<xs:element name="NetworkInteroperability" type="NetworkInteroperability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="SyntacticInteroperability" type="SyntacticInteroperability" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Complexity" type="Complexity"/>
	<xs:complexType name="Complexity">
		<xs:annotation>
			<xs:documentation>Concerns about our understanding of the behavior of CPS due to the richness and heterogeneity of interactions among its components, such as existence of legacy and the variety of interfaces.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="ComponentInventory" type="ComponentInventory"/>
	<xs:complexType name="ComponentInventory">
		<xs:annotation>
			<xs:documentation>Concerns about component inventory or 'bill of materials' (BoM) and versioning.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="CompositionAspect" type="CompositionAspect"/>
	<xs:complexType name="CompositionAspect">
		<xs:annotation>
			<xs:documentation>Concerns about the ability to compute selected properties of a component assembly from the properties of its components. Compositionality requires components that are composable: they do not change their properties in an assembly. Timing composability is particularly difficult.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Aspect">
				<xs:sequence>
					<xs:element name="Adaptability" type="Adaptability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Complexity" type="Complexity" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Constructivity" type="Constructivity" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Discoverability" type="Discoverability" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Conceptualization" type="Conceptualization"/>
	<xs:complexType name="Conceptualization">
		<xs:annotation>
			<xs:documentation>The conceptualization facet is the set of activities, including requirements development and requirements analysis and others, that produce as an output the set of properties of the system under consideration, or the Model of the CPS.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Facet">
				<xs:sequence>
					<xs:element name="ConceptualizationActivity" type="ConceptualizationActivity" minOccurs="1" maxOccurs="1"/>
					<xs:element name="ConceputalizationArtifacts" type="ConceputalizationArtifacts" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Concern" type="Concern"/>
	<xs:complexType name="Concern" abstract="true">
		<xs:annotation>
			<xs:documentation>Dimensions of conceiving, realizing and assuring a CPS through the lifecycle.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Property" type="Property" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Confidentiality" type="Confidentiality"/>
	<xs:complexType name="Confidentiality">
		<xs:annotation>
			<xs:documentation>Concerns about preserving authorized restrictions on access and disclosure. </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Configuration" type="Configuration"/>
	<xs:complexType name="Configuration">
		<xs:annotation>
			<xs:documentation>Concerns about the management of the configuration of data producing entities</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="ConfigurationForLeastFunctionality" type="ConfigurationForLeastFunctionality"/>
	<xs:complexType name="ConfigurationForLeastFunctionality">
		<xs:annotation>
			<xs:documentation>Concerns about minimizing the configuration needed.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="ConfigurationManagement" type="ConfigurationManagement"/>
	<xs:complexType name="ConfigurationManagement">
		<xs:annotation>
			<xs:documentation>Concerns about managing of configuration, versions, etc., including identifying these and their relationships.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Constructivity" type="Constructivity"/>
	<xs:complexType name="Constructivity">
		<xs:annotation>
			<xs:documentation>Concerns about the ability to combine CPS modular components (hardware, software, and data) to satisfy user requirements.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Controllability" type="Controllability"/>
	<xs:complexType name="Controllability">
		<xs:annotation>
			<xs:documentation>Ability of a CPS to control a property of a physical thing. There are many challenges to implementing control systems with CPS including the non-determinism of cyber systems, the uncertainty of location, time and observations or actions, their reliability and security, and complexity. Concerns related to the ability to modify a CPS or its function, if necessary.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Cost" type="Cost"/>
	<xs:complexType name="Cost">
		<xs:annotation>
			<xs:documentation>Concerns related to the direct and indirect investment or monetary flow or other resources required by the CPS throughout its lifecycle. </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Cybersecurity" type="Cybersecurity"/>
	<xs:complexType name="Cybersecurity">
		<xs:annotation>
			<xs:documentation>Concerns about cyber-security, which encompasses confidentiality, integrity and availability.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence>
					<xs:element name="Availability" type="Availability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Confidentiality" type="Confidentiality" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Integrity" type="Integrity" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DataAspect" type="DataAspect"/>
	<xs:complexType name="DataAspect">
		<xs:annotation>
			<xs:documentation>Concerns about data interoperability including data semantics, identity, operations on data, relationships between data, and velocity of data.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Aspect">
				<xs:sequence>
					<xs:element name="DataSemantics" type="DataSemantics" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Identity" type="Identity" minOccurs="0" maxOccurs="1"/>
					<xs:element name="OperationsOnData" type="OperationsOnData" minOccurs="0" maxOccurs="1"/>
					<xs:element name="RelationshipsBetweenData" type="RelationshipsBetweenData" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Velocity" type="Velocity" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DataIntegrity" type="DataIntegrity"/>
	<xs:complexType name="DataIntegrity">
		<xs:annotation>
			<xs:documentation>Concerns about the integrity of the data elements</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DataModels" type="DataModels"/>
	<xs:complexType name="DataModels">
		<xs:annotation>
			<xs:documentation>Concerns about the structure of data/information</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DataQuality" type="DataQuality"/>
	<xs:complexType name="DataQuality">
		<xs:annotation>
			<xs:documentation>Concerns about associated quality metrics of data</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DataSemantics" type="DataSemantics"/>
	<xs:complexType name="DataSemantics">
		<xs:annotation>
			<xs:documentation>Concerns related to the agreed and shared meaning(s) of data held within, generated by, and transiting a system. </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence>
					<xs:element name="DataModels" type="DataModels" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Granularity" type="Granularity" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Meaning" type="Meaning" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Deployability" type="Deployability"/>
	<xs:complexType name="Deployability">
		<xs:annotation>
			<xs:documentation>Concerns about the ease and reliability with which a CPS can be brought into productive use.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DeviceIdentification" type="DeviceIdentification"/>
	<xs:complexType name="DeviceIdentification">
		<xs:annotation>
			<xs:documentation>Concerns about identification of data representing devices</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Directionality" type="Directionality"/>
	<xs:complexType name="Directionality">
		<xs:annotation>
			<xs:documentation>Concerns about directional/non-directional relationships between data</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Discoverability" type="Discoverability"/>
	<xs:complexType name="Discoverability">
		<xs:annotation>
			<xs:documentation>Concerns about the ease and reliability with which a CPS component can be observed and understood (for purposes of leveraging the component’s functionality) by an entity (human, machines). Concerns related to the ease and reliability with which a CPS component’s functions can be ascertained (for purposes of leveraging that functionality) by an entity (human, machines).</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Disposability" type="Disposability"/>
	<xs:complexType name="Disposability">
		<xs:annotation>
			<xs:documentation>Concerns about the impacts that may occur when the CPS is taken physically out of service.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Engineerability" type="Engineerability"/>
	<xs:complexType name="Engineerability">
		<xs:annotation>
			<xs:documentation>Concerns about the ease and reliability with which a CPS design concept can successfully be realized via a structured engineering process.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Enterprise" type="Enterprise"/>
	<xs:complexType name="Enterprise">
		<xs:annotation>
			<xs:documentation>Concerns related to the economic aspects of CPS throughout their lifecycle.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Environment" type="Environment"/>
	<xs:complexType name="Environment">
		<xs:annotation>
			<xs:documentation>Concerns related to the impacts of the engineering and operation of a CPS on the physical world.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="EOL" type="EOL"/>
	<xs:complexType name="EOL">
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence>
					<xs:element name="Disposability" type="Disposability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Preservability" type="Preservability" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Functional" type="Functional"/>
	<xs:complexType name="Functional">
		<xs:sequence/>
	</xs:complexType>
	<xs:element name="FunctionalAspect" type="FunctionalAspect"/>
	<xs:complexType name="FunctionalAspect">
		<xs:annotation>
			<xs:documentation>The Functional Aspect is a set of concerns related to the sensing, computational and actuation functions of the CPS.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Aspect">
				<xs:sequence>
					<xs:element name="Actuation" type="Actuation" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Communication" type="Communication" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Controllability" type="Controllability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Functionality" type="Functionality" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Manageability" type="Manageability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Measurability" type="Measurability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Monitorability" type="Monitorability" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Performance" type="Performance" minOccurs="0" maxOccurs="1"/>
					<xs:element name="PhysicalContext" type="PhysicalContext" minOccurs="0" maxOccurs="1"/>
					<xs:element name="PhysicalProperties" type="PhysicalProperties" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Sensing" type="Sensing" minOccurs="0" maxOccurs="1"/>
					<xs:element name="StatesAndModes" type="StatesAndModes" minOccurs="0" maxOccurs="1"/>
					<xs:element name="Uncertainty" type="Uncertainty" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Functionality" type="Functionality"/>
	<xs:complexType name="Functionality">
		<xs:annotation>
			<xs:documentation>Concerns related to the function that a CPS provides.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Concern">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Fusion" type="Fusion"/>
	<xs:complexType name="Fusion">
		<xs:annotation>
			<xs:documentation>Concerns about the fusion and aggregation of data.</xs:documentation>
		</xs:annotation>
